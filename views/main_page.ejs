<%- include('components/nav_bar') %>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="../static/style/main_page.css" rel="stylesheet" type="text/css">
    <script src="https://kit.fontawesome.com/b1c674bb0b.js" crossorigin="anonymous"></script>
    <title>Home</title>
</head>
<body>
    <div id="search">
        <input id="input">
        <i class="fas fa-search"></i>
    </div>
    <div id="list">
        <% for(item of items) { %>
        <a href="/item/<%= item._id %>">
            <div>
                <img src="/static/images/<%= item._id %><%= item.imgExt %> " alt="">
                <div><%= item.name %> </div>
                <div><%= item.description %> </div>
                <div><%= item.price %> </div>
                <div><%= item.quantity %> </div>
            </div>
        </a>
        <% } %>
    </div>
    <div></div>
    <script type="text/javascript" src="home.js"> 
    </script>
</body>
</html>
<%- include('components/footer') %>